{{> views/header }}
<div class="container">
    <div class="row content">
        <div class="col-sm">
            <span class="light-grey">You want to be a...</span>
            <ul>
                {{#data.careers}}
                    <li><a href="/careers/{{slug}}">{{name}}</a></li>
                {{/data.careers}}
            </ul>
        </div>
        <div class="col-sm">
            <span class="light-grey">Or maybe you just want to learn...</span>
            <ul>
                {{#data.subjects}}
                    <li><a href="/subjects/{{slug}}">{{name}}</a></li>
                {{/data.subjects}}
            </ul>
        </div>
    </div>
</div>
{{> views/footer }}
